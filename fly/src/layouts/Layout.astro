---
export interface Props {
    title: string;
}

const { title } = Astro.props;

Astro.slots
---

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />
        <title>{title}</title>
    </head>
    <body>
        {Astro.slots.has("title") ? <slot name="title"/> : <h1>{title}</h1>}
        <slot />
    </body>
</html>
